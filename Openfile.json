{
  "name":"OpenFile",
  "vendor":"Chang",
  "version":"1.0",
  "vld":[
    {
      "name":"private"
    }
  ],
  "vnfd":[
    {
      "name":"openfile",
      "vendor":"Chang",
      "version":"1.0",
       "lifecycle_event":[
        {
          "event":"INSTANTIATE",
          "lifecycle_events":[
            "install.sh",
            "download.sh"
          ]
        }
      ],
      "virtual_link":[
        {
          "name":"private"
        }
      ],
      "deployment_flavour":[
        {
          "flavour_key":"m1.small"
        }
      ],
      "vdu":[
        {
          "vm_image":[
            "ubuntu16.04"
          ],
          "scale_in_out":3,
          "vnfc":[
            {
              "connection_point":[
                {
                  "floatingIp":"random",
                  "virtual_link_reference":"private",
                  "interfaceId":0
                }
              ]
            }
          ]
        }
      ],
      "type":"server",
      "endpoint":"generic",
      "vnfPackageLocation":"https://github.com/jeni-chang/OpenBaton.git"
    }
  ]
}